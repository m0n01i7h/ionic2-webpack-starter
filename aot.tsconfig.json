{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "sourceMap": false,
    "noEmit": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "baseUrl": ".",
    "paths": {
      "services": [
        "src/services/"
      ],
      "pages": [
        "src/pages/"
      ],
      "common": [
        "src/common/"
      ]
    }
  },
  "exclude": [
    "src/main.production.ts",
    "node_modules"
  ],
  "angularCompilerOptions": {
    "genDir": "ngfactory",
    "debug": true,
    "skipMetadataEmit": true
  }
}